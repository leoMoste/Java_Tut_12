package _17_amigoscode_Java_functional;

enum Gender {
	MALE, FEMALE
}

public class Person {

	private String name;
	private Gender gender;
	
	
	public Person(String name, Gender gender) {
		this.name = name;
		this.gender = gender;
	}


	@Override
	public String toString() {
		return "Person {name=" + name + '\'' + ", gender=" + gender + "}";
	}
	
	
	
	
	
}
